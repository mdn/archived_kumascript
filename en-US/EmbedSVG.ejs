
<pre class="script">
var width = "100%";
var height = 250;
if (!(num.isnan($1) && $1 > 0)) {
    width = $1;
}
if (!(num.isnan($2) && $2 > 0)) {
    height = $2;
}

var found=false;
var allFiles = map.values(page.files);
foreach(var aFile in allFiles) {
  if (string.compare($0, aFile.name, true) == 0) {
    found=true;
    svg(web.text(aFile.uri), width, height);
    break;
  }
}
if (found == false) {
  web.html("<p>Unable to find file '" + $0 + "'.</p>");
}

%>
