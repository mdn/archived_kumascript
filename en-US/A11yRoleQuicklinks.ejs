<%
// Inserts quicklinks for the accessibility roles.
var lang = env.locale;
var url = "";

switch(lang) {
    "de":
        url = "/de/docs/Web/Accessibility/AT-APIs/Gecko/Rollen";
        break;
    "fr":
        url = "/fr/docs/Web/Accessibility/AT-APIs/Gecko/Roles";
        break;
    default:
        url = "/en-US/docs/Web/Accessibility/AT-APIs/Gecko/Roles";
        break;
}
%>
<%-template("QuickLinksWithSubpages", [url])%>
