<%
/* first parameter(optional): Outdated message */
var lang = env.locale;
 
/* assume English */
  
var str = "<strong>The content of this article may be out of date.</strong> " + ($0 || '');
 
if (!string.compare(lang, "de", true)) {
  str = "<strong>Dieser Artikel ist möglicherweise veraltet.</strong> " + ($0 || '');
} else if (!string.compare(lang, "ja", true)) {
  str = "<strong>この記事の内容は古くなっている可能性があります。</strong> " + ($0 || '');
}
%>
<%- template("warning", [str]) %>
