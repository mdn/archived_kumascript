<%
/* one parameter: HTML version */

var link = "HTML " + $0;
if ($0 == "5") {
  link = web.link("/" + env.locale + "/docs/HTML/HTML5", "HTML " + $0);
}

var s_msg = mdn.localString({
  "en-US": ["Introduced in ",""],
  "es": ["Introducido en ",""],
  "fr": ["Ajouté dans ",""],
  "ja": [""," で導入"],
  "zh-CN": ["引进于 ",""]
});

var str = s_msg[0] + link + s_msg[1];
%>
<div class="htmlVersionHeaderTemplate" style="overflow:hidden; margin: 10px 0px; padding: 0px 10px; background-color: rgb(174, 218, 229); font-weight: bold; text-align: center;">
  <p style="margin: 4px 0px;">
    <%- str %>
  </p>
</div>
