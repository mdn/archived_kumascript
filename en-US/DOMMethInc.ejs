<%
// Inserts the text of the specified DOM method's article
//
//  $0 - Method name

var lang = env.locale;

var url = "/en-US/docs/DOM/" + $0;

switch(lang) {
    case "de":
    case "es":
        url = "/" + lang + "/docs/DOM/" + $0;
        break;
}
%>
<span id='m-<%-$0%>'><%-wiki.page(url)%></span>
