<%

// Load the whole page (wiki.page does cache the original page

var page = wiki.page(mdn.localString({
    "en-US": "en-US/docs/CSS/CSS_values_syntax",
    "ja"   : "ja/docs/Web/CSS/CSS_values_syntax",
    "fr"   : "fr/docs/CSS/Syntaxe_des_valeurs_CSS",
    "de"   : "de/docs/CSS/CSS_werte_syntax",
    "zh-CN": "zh-CN/docs/Web/CSS/CSS_values_syntax"
})); // "en-US" is default.

var re = new RegExp("<\\s*span\\s+id\\s*=\\s*"+'"syntax-'+$0+'"'+"[^>]*>(.*)<\\s*/\\s*span\\s*\\>","igm");
var result = re.exec(page);

var lang = env.locale;
var missing = "Syntax not found in DB!";
switch (lang) {
    case "en-us":
        missing = "Syntax not found in DB!";
        break;

    case "de":
        missing = "Syntax nicht in der Datenbank gefunden!";
        break;

    case "zh-cn":
        missing = "该属性的语法尚未添加";
        break;
}

result = (!result) ? missing : result[1];


%><%- result %>
