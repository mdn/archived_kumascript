<%
var lang = env.locale;
var str = "Gecko " + $0 + " note";

switch(lang) {
  case 'de':
    str = "Hinweis zu Gecko " + $0;
    break;
  case 'zh_tw':
  case 'tw':
    str = "Gecko " + $0 + " 備註";
}
%>
<div class="geckoVersionHeading">
<%=str%>
<div style="font-size:9px; position:relative; top:-5px; font-style:italic;"><%=template('geckoRelease', [$0])%></div>
</div>
