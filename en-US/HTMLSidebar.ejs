<%
var currentSection = $0;
var locale = env.locale;

function state(section) {
  if (section == currentSection) {
    return "open";
  }
  return "closed";
}

var text = mdn.localString({
  'en-US': {
    'Tutorials': 'Tutorials:',
    'Introductory': 'Introductory',
    'HTML_Guide': 'HTML developer guide',
    'Intermediate': 'Intermediate',
    'Block-level': 'Block-level elements',
    'Advanced': 'Advanced',
    'Speculative_parsing': 'Optimizing your pages for speculative parsing',
    'Reference': 'References:',
    'HTML_Elements': 'HTML elements',
    'Global_attributes': 'Global attributes',
    'Documentation': 'Documentation:',
    'Useful_lists': 'Useful lists',
    'Index': 'All pages index',
    'HTML_tag': 'Pages tagged "HTML"',
    'Contribute': 'Contribute',
    'Doc_status': 'HTML doc status',
    'The_MDN_project': 'The MDN project',
    'slug_elements': '/docs/Web/HTML/Element',
    'slug_global_attributes': '/docs/Web/HTML/Global_attributes',
  },
});
%>
<section class="Quick_links" id="Quick_Links">
 <ol>
  <li><a href="/<%=locale%>/docs/Web/HTML"><strong><em>HTML</em></strong></a></li>
  <li><a href="/<%=locale%>/docs/Web/HTML/Tutorials"><strong><%=text['Tutorials']%></strong></a></li>
  <li data-default-state="<%=state('Introductory')%>"><a href="#"><%=text['Introductory']%></a>
   <ol>
    <li><a href="/<%=locale%>/docs/Web/Guide/HTML"><%=text['HTML_Guide']%></a></li>
   </ol>
  </li>
  <li data-default-state="<%=state('Intermediate')%>"><a href="#"><%=text['Intermediate']%></a>
   <ol>
    <li><a href="/<%=locale%>/docs/Web/HTML/Block-level_elements"><%=text['Block-level']%></a></li>
   </ol>
  </li>
  <li data-default-state="<%=state('Advanced')%>"><a href="#"><%=text['Advanced']%></a>
   <ol>
    <li><a href="/<%=locale%>/docs/Web/HTML/Optimizing_your_pages_for_speculative_parsing"><%=text['Speculative_parsing']%></a></li>
   </ol>
  </li>
  <li><strong><a href="/<%=locale%>/docs/Web/JavaScript/Reference"><%=text['Reference']%></a></strong></li>
  <li data-default-state="<%=state('Elements')%>"><a href="/<%=locale%>/docs/Web/HTML/Element"><%=text['HTML_Elements']%></a>
   <%-template("ListSubpagesForSidebar", ['/' + locale + text['slug_elements'], 1])%>
  </li>
  <li data-default-state="<%=state('Global_attributes')%>"><a href="/<%=locale%>/docs/Web/HTML/Global_attributes"><%=text['Global_attributes']%></a>
   <%-template("ListSubpagesForSidebar", ['/' + locale + text['slug_global_attributes']])%>
  </li>
  <li><strong><a href="/<%=locale%>/docs/MDN"><%=text['Documentation']%></a></strong></li>
  <li data-default-state="<%=state('Useful lists')%>"><a href="#"><%=text['Useful_lists']%></a>
   <ol>
    <li><a href="/<%=locale%>/docs/Web/HTML/Index"><%=text['Index']%></a></li>
    <li><a href="/<%=locale%>/docs/tag/HTML"><%=text['HTML_tag']%></a></li>
   </ol>
  </li>
  <li data-default-state="<%=state('Contribute')%>"><a href="#"><%=text['Contribute']%></a>
   <ol>
    <li><a href="/<%=locale%>/docs/MDN/Doc_status/HTML"><%=text['Doc_status']%></a></li>
    <li><a href="/<%=locale%>/docs/MDN"><%=text['The_MDN_project']%></a></li>
   </ol>
  </li>
 </ol>
</section>
