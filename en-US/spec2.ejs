<%

/* parameters: */
/* $0 = name of specification */

var spec = $0;
var status;

switch(spec) {
  case 'CSS1':
    status = 'REC';
    break;
  case 'CSS2.1':
    status = 'REC';
    break;
  case 'CSS3 Animations':
    status = 'WD';
    break;
  case 'CSS3 Backgrounds': // Backgrounds & Borders
    status = 'CR';
    break;
  case 'CSS3 Box':
    status = 'WD';
    break;
  case 'CSS3 Cascade':
    status = 'WD';
    break;
  case 'CSS3 Colors':
    status = 'REC';
    break;
  case 'CSS3 Conditional':
    status = 'WD';
    break;
  case 'CSS3 Content':
    status = 'WD';
    break;
  case 'CSS3 Flexbox':
    status = 'WD';
    break;
  case 'CSS3 Fonts':
    status = 'WD';
    break;
  case 'CSS3 Grid':
    status = 'WD';
    break;
  case 'CSS3 Images':
    status = 'WD';
    break;
  case 'CSS3 Lists':
    status = 'WD';
    break;
  case 'CSS3 Media Queries':
    status = 'CR';
    break;
  case 'CSS3 Multicol':
    status = 'CR';
    break;
  case 'CSS3 Namespaces':
    status = 'REC';
    break;
  case 'CSS3 Paged Media':
    status = 'WD';
    break;
  case 'CSS3 Selectors':
    status = 'REC';
    break;
  case 'CSS3 Text':
    status = 'WD';
    break;
  case 'CSS3 Transitions':
    status = 'WD';
    break;
  case 'CSS3 UI':
    status = 'WD';
    break;
  case 'CSS3 Values':
    status = 'WD';
    break;
  case 'CSS3 Writing Modes':
    status = 'WD';
    break;
  case 'CSS3 2D Transforms':
    status = 'WD';
    break;
  case 'CSS3 3D Transforms':
    status = 'WD';
    break;
  case 'DOM3 Events':
    status = 'WD';
    break;
  case 'DOM Parsing':
    status = 'WD';
    break;
  case 'Page Visibility API':
    status = 'WD';
    break;
  default:
    status = 'Unknown';
}

switch (status) {
  case 'REC':
    web.text('Recommendation');
    break;
  case 'PR':
    web.text('Proposed Recommendation');
    break;
  case 'CR':
    web.text('Candidate Recommandation');
    break;
  case 'WD':
    web.text('Working Draft');
    break;
  case 'ED':
    web.text('Editor\'s Draft');
    break;
  default:
    web.text('Unknown');
}
</pre>
