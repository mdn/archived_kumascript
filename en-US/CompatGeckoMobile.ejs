<%
var fxVer = '';
if (string.startswith($0, "1.") || !string.compare($0, "1")) {
  fxVer = '1.0';
} else {
  switch($0) {
    case '2':
    case '2.0':
      fxVer = '4.0';
      break;
    case '5':
    case '5.0':
      fxVer = '5.0';
      break;
    case '6':
    case '6.0':
      fxVer = '6.0';
      break;
    case '7':
    case '7.0':
      fxVer = '7.0';
      break;
    case '8':
    case '8.0':
      fxVer = '8.0';
      break;
    case '9':
    case '9.0':
      fxVer = '9.0';
      break;
    case '10':
    case '10.0':
      fxVer = '10.0';
      break;
  case '11.0':
    fxVer = '11.0';
    break;
  case '12':
  case '12.0':
    fxVer = '12.0';
    break;
  case '13':
  case '13.0':
    fxVer = '13.0';
    break;
  case '14':
  case '14.0':
    fxVer = '14.0';
    break;
  case '15':
  case '15.0':
    fxVer = '15.0';
    break;
  case '16':
  case '16.0':
    fxVer = '16.0';
    break;
  case '17':
  case '17.0':
    fxVer = '17.0';
    break;
}
}
fxVer = fxVer + " (" + $0 + ")";
%>
<%= fxVer %>
