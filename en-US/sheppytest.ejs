<%
// Simple test macro to demonstrate fetching JSON from a Github repo and
// returning the resulting JavaScript object.
//
// Parameters:
//  $0  Git subpath (such as "mozilla/kuma/master/contribute.json")
    
    var url = "https://raw.githubusercontent.com/" + $0;
    var data = mdn.fetchJSONResource(url);
    var output = kuma.inspect(data);
%>
<pre class="brush: json">
<%-output%>
</pre>
