<%
var name = ($0.toString()).toLowerCase();
var lang = env.locale;
var basePath = "/" + lang + "/docs/";
var sectionname = "Element";
var dest = basePath + 'Web/HTML/' + sectionname + '/' + name;
var page = wiki.getPage(dest);
var summary = mdn.escapeQuotes(page.summary);

switch(lang) {
    case "ja":
    case "en-US":
    case "fr":
        break;
    case 'es':
        dest = basePath + 'HTML/Elemento/' + name;
        break;
    default:        // Using the old path for now
        dest = basePath + 'HTML/Element/' + name;
        break;
}

%><a href="<%- dest %>" title="<%-summary%>"><code>&lt;<%- name %>&gt;</code></a>
