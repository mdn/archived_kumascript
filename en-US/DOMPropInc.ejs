<%
// Inserts the text of the specified DOM property's article
//
//  $0 - Property name

var lang = env.locale;

var url = "/en-US/docs/DOM/" + $0;

switch(lang) {
    case "de":
    case "es":
        url = "/" + lang + "/en-US/docs/DOM/" + $0;
        break;
}
%>
<span id='p-<%-$0%>'><%-wiki.page(url)%></span>
