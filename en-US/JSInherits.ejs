<%
/* This automatically links to the method or property of the prototype of the specified global object.
 * Syntax:
 * {{JSInherits("globalObject", "inheritType","inherited",...)}}
 * globalObject - the name of the global object whose prototype properties/methods are being passed on
 * inheritType - Properties or Methods. Make sure you capitalize it and use the plural.
 * inherited - the name of the property or method being inherited.
*/
var str = '';

if ($2) {
    str = '<div style="border: solid #ccc 1px; background: #eee; padding: 0px 10px 0px 10px; margin: 10px 0px 10px 0px;"><b>';

    var links = [];

    switch(env.locale) {
      case 'fr':
        str += $1 + ' héritées de ' + web.link(wiki.uri(env.locale + '/docs/JavaScript/Référence_JavaScript/Objets_globaux/' + $0 + '/prototype'), $0 + '.prototype');
        for (var i = 2; i < arguments.length; i++) {
            links.push(web.link(wiki.uri(env.locale + '/docs/JavaScript/Référence_JavaScript/Objets_globaux/' + $0 + '/' + arguments[i]), arguments[i]));
        }
        break;
      case 'ja':
        str += web.link(wiki.uri(env.locale + '/docs/JavaScript/Reference/Global_Objects/' + $0 + '/prototype'), $0 + '.prototype') + ' から継承される' + $1;
        for (var i = 2; i < arguments.length; i++) {
            links.push(web.link(wiki.uri(env.locale + '/docs/JavaScript/Reference/Global_Objects/' + $0 + '/' + arguments[i]), arguments[i]));
        }
        break;
      case 'pl':
        str += $1 + ' dziedziczone z ' + web.link(wiki.uri(env.locale + '/docs/Dokumentacja_języka_JavaScript_1.5/Obiekty/' + $0 + '/prototype'), $0 + '.prototype');
        for (var i = 2; i < arguments.length; i++) {
            links.push(web.link(wiki.uri(env.locale + '/docs/Dokumentacja_języka_JavaScript_1.5/Obiekty/' + $0 + '/' + arguments[i]), arguments[i]));
        }
        break;
      default:
        str += $1 + ' inherited from ' + web.link(wiki.uri(env.locale + '/docs/JavaScript/Reference/Global_Objects/' + $0 + '/prototype'), $0 + '.prototype');
        for (var i = 2; i < arguments.length; i++) {
            links.push(web.link(wiki.uri(env.locale + '/docs/JavaScript/Reference/Global_Objects/' + $0 + '/' + arguments[i]), arguments[i]));
        }
        break;
    }
    str += '</b><br><small>' + links.join(', ') + '</small></div>';
}
%>
<%- str %>
