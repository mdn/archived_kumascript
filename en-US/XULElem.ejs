<%
var lang = env.locale;

var destOldType = lang + '/XUL:' + $0;
var destNewType = lang + '/XUL/' + $0;

if (wiki.pageExists(destNewType)) {
  %><code><%-(web.link(wiki.uri(destNewType), $0))%></code><%
} else if (wiki.pageExists(destOldType)) {
  %><code><%-(web.link(wiki.uri(destOldType), $0))%></code><%
} else {
  var targeturi = "/docs/new?slug=" + destNewType;
  %><code><a rel="internal" href="<%=targeturi%>" class="new"><%=$0%></a></code><%
}
%>
