
<pre class="script">
/* get a page's language (Don't use page.language!) */
var u = Uri.Parts(Page.uri);
var lang = u.path[0];
if(string.Contains(lang, "project") || string.Contains(lang, "Project")) {
 lang = string.Substr(lang, 8);
}

/* if no $1 then link to constants */
var constantsTable = 'Constants';
if ($1) {
    constantsTable = $1;
}

/* check $0#$1 exists */
if(wiki.pageExists(lang + '/XPCOM_Interface_Reference/' + $0)) {
    <code>(web.link(wiki.uri(lang + '/XPCOM_Interface_Reference/' + $0) + '#' + constantsTable, $0 + '.' + constantsTable))</code>;
} else if(wiki.pageExists(lang + '/' + $0)) {
    <code>(web.link(wiki.uri(lang + '/' + $0) + '#' + constantsTable, $0 + '.' + constantsTable))</code>;
} else {
    <code><a rel=('internal') href=(wiki.uri(lang + '/XPCOM_Interface_Reference/' + $0) + '#' + constantsTable) class=('new')>$0 + '.' + constantsTable</a></code>;
}</pre>
